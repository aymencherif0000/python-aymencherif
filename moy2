moy=float(input("donner la moyenne :"))
if 10<=moy<12:
    print("passable")
elif 12 <=moy<14:
    print("assez bien")
elif 14 <=moy<16:
    print("bien")
elif moy>=16:
    print("trÃ©s bien")
else :
    print("eureur")
